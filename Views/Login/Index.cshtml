@model ProjectC.Models.Medewerker



<link href="~/styles/style.css" rel="stylesheet" />
@{
    ViewBag.Title = "Login";
     Layout =  null;
}



<body>
    <div class="wrapper">
        <div class="title">
            Ranshuijsen BV
        </div>
        @using (Html.BeginForm("Autherize", "Login", FormMethod.Post))
        {
            <table>
                <tr>
                    <td></td>

                </tr>
                <tr>
                    <div class="field">
                        <input type="text" required, id="gebruikersnaam" , name="Gebruikersnaam" autocomplete="off">
                        <label>Email Address</label>
                    </div>

                </tr>
                <tr>
                    <td></td>
                    <td>@Html.ValidationMessageFor(model => model.gebruikersnaam)</td>
                </tr>
                <tr>
                    <div class="field">
                        <input type="password" required, id="wachtwoord" , name="Wachtwoord" autocomplete="off">
                        <label>Password</label>
                    </div>
                </tr>
                <tr>
                    <td></td>
                    <td>@Html.ValidationMessageFor(model => model.wachtwoord)</td>
                </tr>
                <tr>
                    <td></td>
                    <div class="field">
                        <input type="submit" value="Login">
                    </div>
                </tr>
            </table>}

    </div>
</body>


